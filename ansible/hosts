# EC2 Webservers hosting artifactory
[webservers]
server1 ansible_host=172.31.182.129 ansible_ssh_private_key_file=./my_rsa_key
server2 ansible_host=172.31.198.222 ansible_ssh_private_key_file=./my_rsa_key

[webservers:vars]
ansible_ssh_common_args='-o UserKnownHostsFile=/dev/null'

# Load balancer
[loadbalancer]

