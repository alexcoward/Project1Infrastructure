# EC2 Webservers hosting artifactory
[webservers]
server1 ansible_host=172.31.160.190 ansible_ssh_private_key_file=/home/ubuntu/my_rsa_key ansible_ssh_user=ubuntu
server2 ansible_host=172.31.208.120 ansible_ssh_private_key_file=/home/ubuntu/my_rsa_key ansible_ssh_user=ubuntu

[webservers:vars]
ansible_ssh_common_args='-o UserKnownHostsFile=/dev/null'

# Load balancer
[loadbalancer]

# Bastion Server
[bastion]
