# EC2 Webservers hosting artifactory
[webservers]
server1 ansible_host=ip-172-31-182-129.us-west-2.compute.internal ansible_ssh_private_key_file=/home/ubuntu/my_rsa_key
server2 ansible_host=ip-172-31-198-222.us-west-2.compute.internal ansible_ssh_private_key_file=/home/ubuntu/my_rsa_key

[webservers:vars]
ansible_ssh_common_args='-o UserKnownHostsFile=/dev/null'

# Load balancer
[loadbalancer]

# Bastion Server
[bastion]
